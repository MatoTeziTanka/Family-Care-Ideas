# EditorConfig is awesome: https://EditorConfig.org

# top-most EditorConfig file
root = true

[*]
# Indentation settings
indent_style = space
indent_size = 2

# Line ending settings
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[*.md]
trim_trailing_whitespace = false

[*.py]
indent_size = 4

[*.java]
indent_size = 4

[*.go]
indent_size = 4

[*.rs]
indent_size = 4
